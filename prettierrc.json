{
    "plugins": ["prettier-plugin-organize-imports"],
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 120,
    "tabWidth": 4,
    "useTabs": true,
    "semi": false,
    "jsxSingleQuote": true,
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "endOfLine": "auto"
}